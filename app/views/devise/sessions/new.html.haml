= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "form-signin", role: "form" } ) do |f|
  = devise_error_messages!
  %h2.form-signin-heading Sign in
  = f.email_field :email, autofocus: true, class: "form-control", placeholder: "Email address", style: "width:300px;margin: 0 auto;"
  = f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Password", style: "width:300px;margin: 0 auto;"
  / = f.check_box :remember_me
  / = f.label :remember_me
  = f.submit "Sign in", class: "btn btn-lg btn-primary btn-block"
= render "devise/shared/links"

  